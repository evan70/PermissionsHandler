{
    "name": "umpirsky/composer-permissions-handler",
    "description": "Composer script handling directories permissions by making them writable both by the web server and the command line user.",
    "keywords": [
        "permissions handling"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Saša Stamenković",
            "email": "umpirsky@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "symfony/process": "^2.3 || ^3.0 || ^4.0 || ^5.0"
    },
    "require-dev": {
        "composer/composer": "^2.2.4",
        "phpspec/phpspec": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Umpirsky\\": "src",
            "Spec\\": "spec"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "branch-alias": {
        "dev-master": "1.0-dev"
    }
}
